{
  "env_list": {
    "sim_base": {
      "python_version": "3.10",
      "conda_packages": [
        "pip"
      ],
      "pip_packages": [
        "gplearn",
        "pysr",
        "pyoperon"
      ],
      "channels": [
        "conda-forge"
      ],
      "comments": "测试环境，目前可以运行gplearn, pysr, pyoperon",
      "post_install_commands": [
        "pip install -e ."
      ]
    },
    "sim": {
      "python_version": "3.10",
      "conda_packages": [
        "pip",
        "scikit-learn"
      ],
      "pip_packages": [],
      "comments": "这是工具箱的基环境，里面不装任何的依赖包"
    },
    "sim_e2esr": {
      "python_version": "3.7",
      "conda_packages": [
        "pip"
      ],
      "channels": [
        "conda-forge",
        "defaults"
      ],
      "pip_packages": [
        "numpy==1.21.5",
        "scipy==1.7.3",
        "scikit-learn==1.0.2",
        "pandas==1.3.5",
        "sympy==1.10.1",
        "torch==1.11.0",
        "torchaudio==0.11.0",
        "torchvision==0.12.0",
        "tqdm==4.64.0",
        "pyyaml==6.0",
        "requests==2.27.1"
      ],
      "post_install_commands": [
        "pip install -e .",
        "wget https://dl.fbaipublicfiles.com/symbolicregression/model1.pt -O ./scientific_intelligent_modelling/algorithms/e2esr_wrapper/e2esr/model1.pt"
      ],
      "comments": "e2e-symbolic-regression环境"
    },
    "sim_llmsr": {
      "python_version": "3.11.7",
      "conda_packages": [
        "pip"
      ],
      "channels": [
        "conda-forge"
      ],
      "pip_packages": [
        "numpy",
        "litellm",
        "scipy"
      ],
      "post_install_commands": [
        "pip install -e .",
        "pip install -r ./scientific_intelligent_modelling/algorithms/llmsr_wrapper/llmsr/requirements.txt"
      ]
    },
    "sim_dso": {
      "python_version": "3.7",
      "conda_packages": [
        "pip"
      ],
      "channels": [
        "conda-forge"
      ],
      "pip_packages": [
        "numpy"
      ],
      "post_install_commands": [
        "pip install -e .",
        "pip install -e ./scientific_intelligent_modelling/algorithms/dso_wrapper/dso/dso"
      ]
    },
    "sim_tpsr": {
      "python_version": "3.9",
      "conda_packages": [
        "pip",
        "pytorch",
        "numpy",
        "scikit-learn",
        "matplotlib",
        "pandas",
        "sympy"
      ],
      "channels": [
        "conda-forge",
        "pytorch"
      ],
      "pip_packages": [
        "timeout-decorator",
        "numexpr",
        "sympytorch"
      ],
      "comments": "TPSR (Transformer-based Planning for Symbolic Regression)环境",
      "post_install_commands": [
        "pip install -e .",
        "pip install -r ./scientific_intelligent_modelling/algorithms/tpsr_wrapper/tpsr/requirements.txt"
      ]
    },
    "sim_qLattice": {
      "python_version": "3.10",
      "conda_packages": [
        "pip"
      ],
      "channels": [
        "conda-forge"
      ],
      "pip_packages": [
        "numpy",
        "pandas",
        "scikit-learn",
        "sympy",
        "feyn"
      ],
      "post_install_commands": [
        "pip install -e ."
      ],
      "comments": "Feyn/Qlattice 符号建模环境"
    }
