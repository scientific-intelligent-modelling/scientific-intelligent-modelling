{
  "auto_env_creation": true,
  "subprocess_timeout": 3600,
  "memory_limit": 16000,
  "tool_mapping": {
    "gplearn": {
      "env": "sim_base",
      "regressor": "GPLearnRegressor"
    },
    "pysr": {
      "env": "sim_base",
      "regressor": "PySRRegressor"
    },
    "pyoperon": {
      "env": "sim_base",
      "regressor": "OperonRegressor"
    },
    "llmsr": {
      "env": "sim_llmsr",
      "regressor": "LLMSRRegressor"
    },
    "dso": {
      "env": "sim_dso",
      "regressor": "DSORegressor"
    },
    "tpsr": {
      "env": "sim_tpsr",
      "regressor": "TPSRRegressor"
    },
    "e2esr": {
      "env": "sim_e2esr",
      "regressor": "E2ESRRegressor"
    },
    "QLattice": {
      "env": "sim_qLattice",
      "regressor": "QLatticeRegressor"
    }
  }
}